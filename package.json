{
	"name": "sveltekit-tailwind-components",
	"description": "Opinionated yet customizable tailwind components",
	"keywords": [
		"svelte",
		"svelte3",
		"sveltekit",
		"svelte-components",
		"sveltejs",
		"tailwindcss"
	],
	"license": "MIT",
	"version": "0.0.42",
	"repository": {
		"type": "git",
		"url": "https://github.com/N00nDay/sveltekit-tailwind-components"
	},
	"homepage": "https://github.com/N00nDay/sveltekit-tailwind-components",
	"scripts": {
		"dev": "vite dev --open --host --port 5555",
		"build": "vite build",
		"package": "svelte-kit package;",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"package:patch": "npm version patch && svelte-kit package",
		"package:minor": "npm version minor && svelte-kit package",
		"package:major": "npm version major && svelte-kit package",
		"prepare:git": "npm run format && npm run lint && git add -u"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.7",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.36.2",
		"@typescript-eslint/parser": "^5.36.2",
		"autoprefixer": "^10.4.8",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.0",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.16",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.2",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"dayjs": "^1.11.5",
		"svrollbar": "^0.12.0"
	},
	"peerDependencies": {
		"svelte": "^3.50.0"
	},
	"exports": {
		".": "./index.js",
		"./components/cards": "./components/cards/index.js",
