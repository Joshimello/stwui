{
	"name": "sveltekit-tailwind-components",
	"description": "Opinionated yet customizable svelte-tailwind components",
	"keywords": [
		"svelte",
		"svelte3",
		"sveltekit",
		"svelte-components",
		"sveltejs",
		"tailwindcss"
	],
	"license": "MIT",
	"version": "0.0.46",
	"repository": {
		"type": "git",
		"url": "https://github.com/N00nDay/sveltekit-tailwind-components"
	},
	"homepage": "https://github.com/N00nDay/sveltekit-tailwind-components",
	"scripts": {
		"dev": "vite dev --open --host --port 5555",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"package:patch": "npm version patch && svelte-package",
		"package:minor": "npm version minor && svelte-package",
		"package:major": "npm version major && svelte-package",
		"prepare:git": "npm run format && npm run lint && git add -u"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.3",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.7",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.37.0",
		"@typescript-eslint/parser": "^5.37.0",
		"autoprefixer": "^10.4.9",
		"eslint": "^8.23.1",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.50.1",
		"svelte-check": "^2.9.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.16",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.8.3",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"dayjs": "^1.11.5",
		"svrollbar": "^0.12.0",
		"tailwind-merge": "^1.6.0",
		"tippy.js": "^6.3.7"
	},
	"peerDependencies": {
		"svelte": "^3.50.1"
	},
