{
	"name": "sveltekit-tailwind-components",
	"description": "Opinionated yet customizable tailwind components",
	"keywords": [
		"svelte",
		"svelte3",
		"sveltekit",
		"svelte-components",
		"sveltejs",
		"tailwindcss"
	],
	"license": "MIT",
	"version": "0.0.28",
	"repository": {
		"type": "git",
		"url": "https://github.com/N00nDay/sveltekit-tailwind-components"
	},
	"homepage": "https://github.com/N00nDay/sveltekit-tailwind-components",
	"scripts": {
		"dev": "vite dev --open --host --port 5555",
		"build": "vite build",
		"package": "svelte-kit package;",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"patch-package": "npm version patch && svelte-kit package",
		"minor-package": "npm version minor && svelte-kit package",
		"major-package": "npm version major && svelte-kit package",
		"prepare-git": "npm run format && npm run lint"
	},
	"devDependencies": {
		"@playwright/test": "^1.24.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "^1.0.0-next.66",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.4",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"autoprefixer": "^10.4.8",
		"eslint": "^8.21.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.16",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.13",
		"tailwindcss": "^3.1.8",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.5"
	},
	"type": "module",
	"dependencies": {
		"dayjs": "^1.11.4"
	},
	"peerDependencies": {
		"svelte": "^3.49.0"
	},
	"exports": {
		".": "./index.js",
		"./components/cards": "./components/cards/index.js",
		"./components/button": "./components/button/index.js",
		"./components/swap": "./components/swap/index.js"
	},
	"svelte": "."
}
